\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n\n        <div class=\"media-container-row align-items-center\">\n            <div class=\"col-{{logoSize}}\">\n                <div class=\"media-wrap align-center\">\n                    <span mbr-if=\"showLogo\" class=\"navbar-logo\">\n                        <a href=\"https://electricistasnavarra.es\">\n                            <img src=\"@PROJECT_PATH@/assets/images/logo-electricidad-429x429.png\" alt=\"Electricistas Navarra\">\n                        </a>\n                    </span>\n                </div>\n            </div>\n            <div class=\" col-12 col-sm-{{12-logoSize}} col-md-{{9-logoSize}}\" mbr-if=\"showItems\">\n                <p class=\"mbr-text links mbr-fonts-style d-flex flex-wrap\" mbr-theme-style=\"display-7\" data-app-selector=\".links\">\n                    <a href=\"index.html#header3-2\" class=\"text-white link text-primary\">Inicio</a>\n                    <a href=\"index.html#features6-4\" class=\"text-white link text-primary\">Servicios</a>\n                    <a href=\"#\" class=\"text-white link\">Nosotros</a>\n                    <a href=\"index.html#extPricingTables4-1d\" class=\"text-white link text-primary\">Precios</a>\n                    <a href=\"index.html#contacts2-19\" class=\"text-white link text-primary\">Contacto</a>\n                </p>\n            </div>\n            <div class=\"col-12 col-sm-12 col-md-3\">\n                <div class=\"social-list align-left\" mbr-if=\"showSocial\">\n                    <div class=\"soc-item\" mbr-if=\"socialCount>0\">\n                        <a href=\"https://instagram.com/mobirise\" target=\"_blank\">\n                            <span mbr-icon class=\"socicon-instagram socicon mbr-iconfont mbr-iconfont-social\"></span>\n                        </a>\n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"socialCount>1\">\n                        <a href=\"https://www.facebook.com/pages/Mobirise/1616226671953247\" target=\"_blank\">\n                            <span mbr-icon class=\"socicon-facebook socicon mbr-iconfont mbr-iconfont-social\"></span>\n                        </a>\n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"socialCount>2\">\n                        <a href=\"https://www.youtube.com/c/mobirise\" target=\"_blank\">\n                            <span mbr-icon class=\"socicon-youtube socicon mbr-iconfont mbr-iconfont-social\"></span>\n                        </a>\n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"socialCount>3\">\n                        <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                            <span mbr-icon class=\"socicon-twitter socicon mbr-iconfont mbr-iconfont-social\"></span>\n                        </a>\n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"socialCount>4\">\n                        <a href=\"https://vimeo.com/mobirise\" target=\"_blank\">\n                            <span mbr-icon class=\"socicon-vimeo socicon mbr-iconfont mbr-iconfont-social\"></span>\n                        </a>\n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"socialCount>5\">\n                        <a href=\"https://www.behance.net/Mobirise\" target=\"_blank\">\n                            <span mbr-icon class=\"socicon-behance socicon mbr-iconfont mbr-iconfont-social\"></span>\n                        </a>\n                    </div>\n                </div>\n            </div>\n        </div>\n        \n        <div class=\"footer-lower\"